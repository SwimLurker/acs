<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>TR069 ACS</display-name>
    <servlet>
        <description>TR069 Servlet</description>
        <display-name>TR069 Servlet</display-name>
        <servlet-name>TR069Servlet</servlet-name>
        <servlet-class>org.slstudio.acs.engine.TR069Servlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>TR069Servlet</servlet-name>
        <url-pattern>/tr069</url-pattern>
    </servlet-mapping>
</web-app>